  <body>
    <p id="notice"></p>
    <h1>Books</h1>
    <% @books.each do |book| %>
    <table>
      <tr>
        <th>Title</th>
        <th>Body</th>
        <th colspan="3"></th>
      </tr>

      <tr>
        <td><%= link_to book.title %></td>
        <td><%= link_to book.body %></td>
        <td><%= link_to "Show", book_path(book.id) %><%= link_to "Edit",book_path(list.id) %><%= link_to "Destroy",book_path(book.id) %></td>
      </tr>
    </table>

    <h2>New book</h2>

    <%= form_with model: @book do |f| %>

    <p>Title</p>
    <%= f.text_field :title %>

    <p>Body</p>
    <%= f.text_area :body %>

    <%= f.submit 'Create Book' %>

    <% end %>

  </body>
</html>